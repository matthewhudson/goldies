{
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "include": ["./src"],
    "exclude": ["node_modules"]
  },
  "recurseDepth": 10,
  "opts": {
    "encoding": "utf8",
    "verbose": true,
    "recurse": true,
    "destination": "./docs/",
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "homepageTitle": "goldies",
      "title": "goldies",
      "default_theme": "light",
      "displayModuleHeader": true,
      "includeFilesListInHomepage": true
    }
  }
}
